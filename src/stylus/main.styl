@import url('https://fonts.googleapis.com/css?family=Nunito:400,700|Roboto+Condensed:400,700|Roboto+Slab:400,700')

$body-font-family = 'Nunito', Roboto, Arial, sans-serif
// $heading-font-family = 'Roboto Slab', Roboto, Arial, sans-serif
$alternate-font-family = 'Roboto Condensed', Roboto, Arial, sans-serif
$font-family-fontawesome = FontAwesome, 'Helvetica Neue', Helvetica, Arial, sans-serif

@import '~vuetify/src/stylus/main'

h1, h2, h3, h4, h5, h6
  // font-family: $header-font-family
  font-weight: normal

.alt-text
  font-family: $alternate-font-family
  font-weight: bold
  text-transform: uppercase
  font-size: 16px
  letter-spacing: 0.125em
